<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>FullCalendar in .NET Core</title>
    <link href="~/lib/jquery.fullcalendar/3.9.0/content/Content/fullcalendar.css" rel="stylesheet" />
    <environment include="Development">
        <link rel="stylesheet" href="~/css/calendar.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="~/css/calendar.min.css" asp-append-version="true" />
    </environment>
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
</head>
<body>
    @RenderBody()
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/lib/moment.js/2.22.2/Content/Scripts/moment.js"></script>
    <script src="~/lib/jquery.fullcalendar/3.9.0/content/Scripts/fullcalendar.js"></script>
    <link href="~/lib/qtip2/2.2.1/content/Scripts/qTip/jquery.qtip.css" rel="stylesheet" />
    <script src="~/lib/qtip2/2.2.1/content/Scripts/qTip/jquery.qtip.js"></script>
    <link href="~/lib/pyramius.flatpickr/4.3.2/content/Content/flatpickr.min.css" rel="stylesheet" />
    <script src="~/lib/pyramius.flatpickr/4.3.2/content/Scripts/flatpickr.js"></script>
    <script src="~/lib/axios/0.16.2/Content/Scripts/axios.js"></script>

    <environment include="Development">
        <script src="~/js/calendar.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/calendar.min.js" asp-append-version="true"></script>
    </environment>

    @RenderSection("Scripts", required: false)
</body>
</html>
